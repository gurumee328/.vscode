{
    "configurations": [
        {
            "name": "Win32",
            "intelliSenseMode": "gcc-x64",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "__GNUC__=8",
                "__stdcall=attribute((stdcall))",
                "__cdecl=__attribute__((__cdecl__))",
                "__cplusplus=201703L"
            ],
            "includePath": [
                "${workspaceFolder}/include",
                "/usr/include/**"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/include",
                    "/usr/include/**"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "/usr/bin/gcc.exe"
        }
    ],
    "version": 4
}